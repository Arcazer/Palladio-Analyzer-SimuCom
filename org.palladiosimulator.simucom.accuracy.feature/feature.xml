<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.simucom.accuracy.feature"
      label="Palladio Accuracy Feature"
      version="1.0.0.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://www.palladiosimulator.org">
      This feature installed the Palladio Component Model based on
EMF. With this feature it is possible to model, persist and visualize
instances of the Palladio Component Model. A basic graphical
Service Effect Editor is also provided.
   </description>

   <requires>
      <import plugin="org.palladiosimulator.analyzer.workflow" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.workflow.mdsd" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.errorhandling" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.fzi.se.quality" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.palladiosimulator.analyzer.accuracy" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.pcm" version="4.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.palladiosimulator.analyzer.accuracy"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.analyzer.accuracy.tests"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
